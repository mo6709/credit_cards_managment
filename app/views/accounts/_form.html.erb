<div class="account_form">
	<%= form_for(account, url: "/users/#{account.user_id}/accounts") do |account_builder|%>
	  <p><%= account_builder.label :account_number %></p>
	  <p><%= account_builder.text_field :account_number%></p>
	  <p><%= account_builder.label :credit_line %></p>
	  <p><%= account_builder.number_field :credit_line %></p>
	  <p><%= account_builder.label :due_date %></p>
	  <p><%= account_builder.date_field :due_date%></p>
	  <p><%= account_builder.label :open_date %></p>
	  <p><%= account_builder.date_field :open_date %></p>
	  <p><%= account_builder.submit %></p>
	<% end %>
</div>